{
    "#regular-text":
    {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "16",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        },
        "misc":
        {
            "text_shadow_size": "0"
        }
    },
    "button":
    {
        "colours":
        {
            "normal_bg": "#efefef",
            "hovered_bg": "#d1d1d1"
        },
        "font":
        {
            "name": "roboto",
            "size": "24",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        }
    },
    "label":
    {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "64",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        },
        "misc":
        {
            "text_shadow_size": "0",
            "text_shadow_offset": "0,0",
            "text_vert_alignment": "bottom"
        }
    },
    "@title": {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "72",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        },
        "misc":
        {
            "text_shadow_size": "0",
            "text_vert_alignment": "bottom"
        }
    },
    "@heading": {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "36",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        },
        "misc":
        {
            "text_shadow_size": "0",
            "text_vert_alignment": "bottom"
        }
    },
    "@regular-text-bottom": {
        "prototype": "#regular-text",
        "misc":
        {
            "text_vert_alignment": "bottom"
        }
    },
    "@regular-text-center": {
        "prototype": "#regular-text",
        "misc":
        {
            "text_vert_alignment": "center"
        }
    },
    "@large-text-center": {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "64",
            "regular_resource": {
                "package": "assets.fonts",
                "resource": "Roboto-Regular.ttf"
            }
        },
        "misc":
        {
            "text_shadow_size": "0",
            "text_vert_alignment": "center"
        }
    },
    "@small-button": {
        "font": {
            "name": "roboto",
            "size": "16"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "4",
            "border_width" : "0"
        }
    },
    "@large-button": {
        "font": {
            "name": "roboto",
            "size": "24"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "15",
            "border_width" : "0",
            "shadow_width": "0"
        }
    },
    "#paused": {
        "font": {
            "name": "roboto",
            "size": "20"
        },
        "misc":
        {
            "text_vert_alignment": "center"
        }
    },
    "#close-button": {
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "close-button.png"
            }
        }
    },
    "#pause-button": {
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "pause-button.png"
            }
        }
    },
    "#up-arrow": {
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "up-arrow.png"
            }
        }
    },
    "#down-arrow": {
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "down-arrow.png"
            }
        }
    },
    "#locked-button": {
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "ratio-lock.png"
            }
        }
    },
    "#unlocked-button": {
        "colours": {
            "normal_bg": "#787878"
        },
        "images": {
            "normal_image": {
                "package": "assets.images",
                "resource": "ratio-lock.png"
            }
        }
    }
}