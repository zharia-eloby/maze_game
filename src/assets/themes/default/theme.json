{
    "#font-prototype":
    {
        "font":
        {
            "name": "roboto",
            "size": "1",
            "regular_resource": 
            {
                "package": "assets.fonts",
                "resource": "PottaOne-Regular.ttf"
            }
        }
    },
    "#regular-text":
    {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "prototype": "#font-prototype",
        "misc":
        {
            "text_shadow_size": "0"
        }
    },
    "#small-regular-text": {
        "prototype": "#regular-text",
        "font":
        {
            "name": "roboto",
            "size": "16"
        }
    },
    "@small-text-bottom": 
    {
        "prototype": "#small-regular-text",
        "misc":
        {
            "text_vert_alignment": "bottom"
        }
    },
    "@small-text-center": 
    {
        "prototype": "#small-regular-text",
        "misc":
        {
            "text_vert_alignment": "center"
        }
    },
    "#large-regular-text": {
        "prototype": "#regular-text",
        "font":
        {
            "name": "roboto",
            "size": "64"
        }
    },
    "button":
    {
        "colours":
        {
            "normal_bg": "#efefef",
            "hovered_bg": "#d1d1d1",
            "active_bg": "#c0c0c0",
            "disabled_bg": "#909090",
            "normal_text": "#000000",
            "hovered_text": "#000000",
            "active_text": "#000000"
        },
        "prototype": "#font-prototype",
        "font":
        {
            "name": "roboto",
            "size": "24"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "border_width" : "0",
            "shadow_width": "0"
        }
    },
    "label":
    {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "prototype": "#font-prototype",
        "font":
        {
            "name": "roboto",
            "size": "64"
        },
        "misc":
        {
            "text_shadow_size": "0",
            "text_shadow_offset": "0,0",
            "text_vert_alignment": "bottom"
        }
    },
    "#player" : 
    {
        "colours":
        {
            "normal_bg": "#ffffff",
            "active_bg": "#ffffff",
            "hovered_bg": "#ffffff",
            "disabled_bg": "#ffffff",
            "selected_bg": "#ffffff"
        },
        "misc":
        {
            "border_width": "0",
            "shadow_width": "0",
            "shape": "ellipse"
        }
    },
    "#wall" :
    {
        "colours":
        {
            "dark_bg": "#000000"
        },
        "misc" :
        {
            "border_width":"0",
            "shadow_width":"0"
        }
    },
    "#background": 
    {
        "colours":
        {
            "dark_bg": "#d8bac6"
        },
        "misc":
        {
            "border_width" : "0",
            "shadow_width" : "0"
        },
        "images": 
        {
            "disabled_image":
            {
                "package": "assets.images",
                "resource": "bg-waves-1.jpg"
            }
        }
        
    },
    "#title": 
    {
        "prototype": "#regular-text",
        "font":
        {
            "name": "roboto",
            "size": "72"
        },
        "misc":
        {
            "text_vert_alignment": "bottom"
        }
    },
    "#startpoint":
    {
        "colours":
        {
            "dark_bg":"#b3b3b3"
        },
        "misc":
        {
            "shadow_width": "0",
            "border_width": "0"
        }
    },
    "#endpoint":
    {
        "colours":
        {
            "dark_bg":"#17d2a3"
