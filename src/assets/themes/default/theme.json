{
    "#general-font":
    {
        "colours":
        {
            "normal_text": "#ffffff"
        },
        "font":
        {
            "name": "roboto",
            "size": "1",
            "regular_resource": 
            {
                "package": "assets.fonts",
                "resource": "ChelseaMarket-Regular.ttf"
            }
        }
    },
    "@small-text":
    {
        "prototype": "#general-font",
